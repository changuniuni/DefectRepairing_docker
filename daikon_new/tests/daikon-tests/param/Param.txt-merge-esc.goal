  /**
   *
   */
  public void work(int pi1, int pi2, ParamType ps1, ParamType ps2)
  {
    pi2 ++;

    ps2 = new ParamType();

    ps1.a ++;
    ps2.a ++;

    
  }

  /*@ ensures this.gs1.a == 10; */
  public Param() {
